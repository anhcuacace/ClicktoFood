<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav"
    app:startDestination="@id/homeFragment">
    <include app:graph="@navigation/mobile_navigation" />
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.tunanh.clicktofood.ui.home.main.HomeFragment"
        android:label="HomeFragment" />
    <fragment
        android:id="@+id/categoryFragment2"
        android:name="com.tunanh.clicktofood.ui.home.category.CategoryFragment"
        android:label="CategoryFragment" />
    <fragment
        android:id="@+id/wishListFragment2"
        android:name="com.tunanh.clicktofood.ui.home.wishlist.WishListFragment"
        android:label="WishListFragment" />
    <fragment
        android:id="@+id/moreFragment"
        android:name="com.tunanh.clicktofood.ui.home.more.MoreFragment"
        android:label="MoreFragment"
        tools:layout="@layout/fragment_more">
        <action
            android:id="@+id/action_moreFragment_to_updateProfileFragment"
            app:destination="@id/updateProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/updateProfileFragment"
        android:name="com.tunanh.clicktofood.ui.home.more.profile.UpdateProfileFragment"
        android:label="UpdateProfileFragment"
        tools:layout="@layout/fragment_update_profile">
        <action
            android:id="@+id/action_updateProfileFragment_to_moreFragment"
            app:destination="@id/moreFragment"
            app:popUpTo="@id/moreFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/mainFragment2"
        android:name="com.tunanh.clicktofood.ui.home.MainFragment"
        android:label="MainFragment" />
</navigation>